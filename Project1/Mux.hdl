/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    /* Logical expression Y=S'A + SB */
    Nand(a=sel , b=sel , out=NotNandOut );
    And(a=a , b=NotNandOut , out=AndAOut );
    And(a=sel , b=b , out=AndBOut );
    Or(a=AndAOut , b=AndBOut , out=out );
    }